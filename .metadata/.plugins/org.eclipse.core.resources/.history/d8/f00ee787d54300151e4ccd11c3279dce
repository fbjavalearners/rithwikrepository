package org.jl;

public class StudentTo {

	private int sid;
	private String sname;
	
	public StudentTo(){
		
		
	}

	public StudentTo(int sid,String sname ){
		
		this.sid=sid;
		this.sname=sname;
	}
	
	
	public int getSid() {
		return sid;
	}

	public void setSid(int sid) {
		this.sid = sid;
	}

	public String getSname() {
		return sname;
	}

	public void setSname(String sname) {
		this.sname = sname;
	}
	
	
	
	
}
